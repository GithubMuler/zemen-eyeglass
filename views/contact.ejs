<section class="contact">

        <h2 style="background-color:orange "> <%= message %></h2>
<h1 class="title"> get in touch </h1>
<div class="row">

        <form method="POST"action="/contact" class="contact-form" >
            <input type="text" placeholder="your name" class="box" name="name" autofocus>
            <input type="number" placeholder="your number" class="box" name="phone">
            <input type="email" placeholder="your email" class="box" name="email" required>
            <input type="test" placeholder="your subject" class="box" name="subject">
            <textarea  placeholder="your message"  cols="30" rows="10" name="message" required></textarea>
            <input type="submit" value="send message" class="btn ">
        </form>


    <div>
